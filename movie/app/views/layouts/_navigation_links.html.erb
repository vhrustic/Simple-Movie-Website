<%# add navigation links to this file %>
<% if current_user %>
    <li><%= link_to current_user.username, profile_path%></li>
    <li><%= link_to "Log out", logout_path, :method => :post %></li>
  <% else %>
    <li><%= link_to "Register", register_path %></li>
    <li><%= link_to "Log in", login_path %></li>
  <% end %>